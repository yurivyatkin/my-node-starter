# editorconfig.org

root = true

[*]
charset = utf-8
insert_final_newline = true
indent_style = space
indent_size = 2
trim_trailing_whitespace = true

[*.md]
trim_trailing_whitespace = false

[package.json]
charset = none
end_of_line = none
insert_final_newline = none
trim_trailing_whitespace = none
indent_style = space
indent_size = 2
